<?xml version="1.0" encoding="utf-8"?>
<form name="AllowedTVs">
  <actionbar bgcolor="#f0f0f0">
    <actionbuttonstyle bgcolor="#f0f0f0"/>
    <sharedactionref id="1"/>
    <sharedactionref id="2"/>
    <sharedactionref id="3"/>
    <sharedactionref id="4"/>
    <sharedactionref id="5"/>
  </actionbar>
  <body>
    <richtext>
      <par>Allowed TVs</par>
      <table>
        <tablerow>
          <tablecell>
            <par>ID</par>
          </tablecell>
          <tablecell>
            <par>
              <field allowmultivalues="false" allowtabout="false" computeaftervalidation="false" dataconnectionfield="data" defaultfocus="false" kind="editable" name="ID" protected="false" showdelimiters="true" sign="false" storelocally="false" type="text" useappletinbrowser="false"/>
            </par>
          </tablecell>
          <tablecell>
            <par>AppleTV item ID</par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell>
            <par>Display Name</par>
          </tablecell>
          <tablecell>
            <par>
              <field allowmultivalues="false" allowtabout="false" computeaftervalidation="false" dataconnectionfield="data" defaultfocus="false" kind="editable" name="tvName" protected="false" showdelimiters="true" sign="false" storelocally="false" type="text" useappletinbrowser="false"/>
            </par>
          </tablecell>
          <tablecell>
            <par/>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell>
            <par>Camera List</par>
          </tablecell>
          <tablecell>
            <par>
              <field allowmultivalues="true" allowtabout="false" computeaftervalidation="false" dataconnectionfield="data" defaultfocus="false" kind="editable" listdisplayseparator="semicolon" listinputseparators="comma semicolon" name="availableCameras" protected="false" showdelimiters="true" sign="false" storelocally="false" type="text" useappletinbrowser="false"/>
            </par>
          </tablecell>
          <tablecell>
            <par>Camera IDs available to the particular TV</par>
          </tablecell>
        </tablerow>
      </table>
    </richtext>
  </body>
</form>